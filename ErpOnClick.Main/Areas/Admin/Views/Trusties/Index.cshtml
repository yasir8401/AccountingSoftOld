@using ErpOnClick.DAL.Models
@{
   
    
}

@section TabName{
   Owner's
}

<div class="card card-default card-outline">

    <div class="card-header">
        <h2 class="card-title">
            Trusties
        </h2>
        <!-- tools box -->
        <div class="card-tools">
            <a href="~/admin/Trusties/edit" type="button" class="btn  btn-sm btn-primary">
                <i class="fas fa-plus"></i> Add New
            </a>
        </div>
        <!-- /. tools -->
    </div>
   



    <div class="card-body">
        <div id="divList"></div>

    </div>
</div>


@section scripts{
    <script>
        $(function () {
            BindGrid();

        });


        function BindGrid() {

            $('#divList').html('<h5 class="margin-0"><p><i class="fa fa-spinner fa-spin fa-fw"></i> Please wait while data is loading...</p></h5>');
            $.ajax({
                url: getApplicationName() +'admin/Trusties/_list',
                data: $('#frmSearch').serialize(),
                success: function (data) {
                    $('#divList').html(data);
                }
            })
        };

    </script>
}

